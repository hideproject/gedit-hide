<?xml version="1.0" ?>
<style-scheme id="wombat-hide" name="Wombat (Hide Plugin Pack)" version="1.0">
	<author/>
	<_description>Wombat theme</_description>
	
	<!-- Pallete -->
	<color name="white" value="#ffffff"/>
	<color name="black" value="#000000"/>
	
	
	<!-- Editor Colors -->
	<style name="current-line" background="#414141"/>
	<style name="cursor" background="#656565"/>
	<style name="text" background="#242424" foreground="#f6f3e8"/>
	<style name="line-numbers" background="black" foreground="#857b6f"/>
	<style name="selection" background="#5b6166" foreground="#f8f8f8"/>
	<style name="bracket-match" background="#857b6f" foreground="#f6f3e8" bold="true"/>
	<style name="search-match" background="#FFFF00" foreground="#857b6f"/>
	
	<!-- Def:* colors -->
	<style name="def:comment" foreground="#99968b" italic="true"/>
	<style name="def:constant" foreground="#e5786d"/>
	<style name="def:string" foreground="#95e454" italic="true"/>
	<style name="def:identifier" foreground="#cae682"/>
	<style name="def:function" foreground="#cae682"/>
	<style name="def:type" foreground="#cae682"/>
	<style name="def:statement" foreground="#8ac6f2"/>
	<style name="def:keyword" foreground="#8ac6f2"/>
	<style name="def:preprocessor" foreground="#e5786d"/>
	<style name="def:number" foreground="#e5786d"/>
	<style name="def:specials" foreground="#e7f6da"/>
	<style name="def:special-char" foreground="#e7f6da"/>
	
	<style name="def:error" background="#CD1818" line-background="#cae682" foreground="white" bold="true"/>
	<style name="def:note" background="#1884CC" foreground="white" bold="true"/>

</style-scheme>

